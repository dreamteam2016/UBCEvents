{{#each model as |event|}}
    <div class="panel panel-default">
        <div class="panel-heading">
            <h3 class="panel-title">{{event.name}}</h3>
        </div>
        <div class="panel-body">
            <p>Address: {{event.building}}</p>
            <p>Phone: {{library.roomNumber}}</p>
        </div>
    </div>
{{/each}}